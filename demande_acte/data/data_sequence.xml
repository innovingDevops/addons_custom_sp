<?xml version="1.0" encoding="utf-8"?>

<odoo>
	<data noupdate="1">
		<!-- Sequences for demande acte -->
		
		<record id="seq_demande_acte" model="ir.sequence">
			<field name="name">Code demande</field>
			<field name="code">code.demande</field>
			<field name="prefix">C-DA/</field>
			<field name="padding">7</field>
			<field name="suffix">/%(y)s</field>
		</record>
		<record id="seq_numero_certificat_vie" model="ir.sequence">
			<field name="name">Num√©ro certificat vie</field>
			<field name="code">numero.certificat.vie</field>
			<field name="prefix"></field>
			<field name="padding">7</field>
			<field name="suffix"></field>
		</record>
		<record id="seq_demande_ponctuelle" model="ir.sequence">
			<field name="name">Demande ponctuelle</field>
			<field name="code">demande.ponctuelle</field>
			<field name="prefix"></field>
			<field name="padding">7</field>
			<field name="suffix">/%(year)s</field>
		</record>
		<record id="seq_caisse" model="ir.sequence">
			<field name="name">Caisse</field>
			<field name="code">demande.acte.caisse</field>
			<field name="prefix">C-</field>
			<field name="padding">7</field>
			<field name="suffix">/%(year)s</field>
		</record>
		<record id="seq_caisse_line" model="ir.sequence">
			<field name="name">Caisse line</field>
			<field name="code">demande.acte.caisse.line</field>
			<field name="prefix">CL-</field>
			<field name="padding">7</field>
			<field name="suffix">/%(year)s</field>
		</record>
	</data>
</odoo>